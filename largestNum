def largest_number(large_list):
    if not large_list:
        return None
    largest = large_list[0]
    for num in large_list:
        if num > largest:
            largest = num
    return largest
my_list = [21,25,65,85,14,60,24,74]
print(f"largest Number in the list is {largest_number(my_list)}::")